<!-- Include the Header -->
<%- include('partials/header') %> 

<div class="wrapper d-flex mx-auto">
  <div class=" container d-flex flex-column">
      <div class="d-flex justify-content-center">
          <div class="text-center">
              <h1 style="font-family: Raleway;">DexApp</h1>
              <h4 style="color: rgba(255, 255, 255, .3); font-family: Raleway;">The helpful Pokemon card lookup tool.</h4>    
          </div>
      </div>
      <div class="d-flex justify-content-center">
          <form class="search-form" onsubmit="return false;">
            <div class="search-wrapper">
              <input class="searchField" type="search" placeholder="Search for a card..." onkeypress="submitOnEnter(event)">
              <span class="search-icon"></span>
            </div>
          </form>
      </div>
  </div>
</div>
<!-- Include the footer -->
<%- include('partials/footer') %> 